<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
  PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
  "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
  

  
<mapper namespace="com.ssafy.web.board.model.mapper.BoardMapper">

	<insert id="registBoard" parameterType="board">
		insert into board(title, content, email)
		values (#{title},#{content},#{email})
	</insert>
	
	<select id="listBoard" resultType="board">
		select * from board;
	</select>
	
	<select id="selectBoard" parameterType="int">
		select * from board where no=#{no};
	</select>
	
</mapper>